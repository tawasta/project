<?xml version="1.0" encoding="utf-8" ?>
<odoo noupdate="1">

    <record id="ir_cron_project_task_autocolor" model="ir.cron" forcecreate="True">
        <field name="name">Project: Auto-color tasks</field>
        <field name="model_id" ref="model_project_project" />
        <field name="state">code</field>
        <field name="code">model.action_cron_compute_task_colors()</field>
        <field name="active" eval="True" />
        <field name="user_id" ref="base.user_root" />
        <field name="interval_number">8</field>
        <field name="interval_type">hours</field>
        <field name="numbercall">-1</field>
        <field eval="False" name="doall" />
    </record>

</odoo>
